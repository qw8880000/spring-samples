<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <!-- Definition for textEditor bean without constructor-arg  -->
    <bean id="textEditor" class="com.samples.AnnotationBasedConfiguration.TextEditor">
    </bean>

    <!-- Definition for textEditor2 bean without constructor-arg  -->
    <bean id="textEditor2" class="com.samples.AnnotationBasedConfiguration.TextEditor2">
    </bean>

    <!-- Definition for textEditor3 bean without constructor-arg  -->
    <bean id="textEditor3" class="com.samples.AnnotationBasedConfiguration.TextEditor3">
    </bean>

    <!-- Definition for spellChecker bean -->
    <bean id="spellChecker" class="com.samples.AnnotationBasedConfiguration.SpellChecker">
    </bean>


</beans>